<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- myBatis 활동방식 설정 -->
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="defaultExecutorType" value="REUSE" />
	</settings>

	<!-- 알리아스 설정 -->
	<typeAliases>
		<typeAlias alias="mVO" type="com.human.gallery.vo.MemberVO"/>
		<typeAlias alias="gVO" type="com.human.gallery.vo.GalleryVO"/>
		<typeAlias alias="page" type="com.human.gallery.util.PageUtil"/>
	<!--  
		<typeAlias alias="bVO" type="com.human.gallery.vo.BoardVO"/>
		<typeAlias alias="fVO" type="com.human.gallery.vo.FileVO"/>
		<typeAlias alias="sVO" type="com.human.gallery.vo.SurveyVO"/>
	-->
	</typeAliases>

</configuration>